{
  "manifestVersion": 1,
  "id": "CiscoSpark",
  "name": "Cisco Spark",
  "version": "0.1.8",
  "publisher": "carlowahlstedt",
  "targets": [
    {
      "id": "Microsoft.VisualStudio.Services"
    }
  ],
  "description": "Posts a message to a Cisco Spark space.",
  "categories": [
    "Collaborate"
  ],
  "tags": [
    "cisco spark",
    "message"
  ],
  "screenshots": [
    {
      "path": "images/screenshot.png"
    }
  ],
  "icons": {
    "default": "images/icon.png"
  },
  "files": [
    {
      "path": "buildtask"
    }
  ],
  "content": {
    "details": {
      "path": "README.md"
    }
  },
  "branding": {
    "color": "rgb(241, 92, 34)"
  },
  "repository": {
    "type": "git",
    "uri": "https://github.com/carlowahlstedt/Spark_VSTS_Task"
  },
  "links": {
    "home": {
      "uri": "https://github.com/carlowahlstedt/Spark_VSTS_Task"
    },
    "repository": {
      "uri": "https://github.com/carlowahlstedt/Spark_VSTS_Task"
    },
    "issues": {
      "uri": "https://github.com/carlowahlstedt/Spark_VSTS_Task/issues"
    }
  },
  "contributions": [
    {
      "id": "CiscoSpark",
      "type": "ms.vss-distributed-task.task",
      "targets": [
        "ms.vss-distributed-task.tasks"
      ],
      "properties": {
        "name": "buildtask"
      }
    }
  ]
}